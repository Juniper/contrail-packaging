
[program:cinder-scheduler]
command=/usr/bin/python /usr/bin/cinder-scheduler --config-file /usr/share/cinder/cinder-dist.conf --config-file /etc/cinder/cinder.conf --logfile /var/log/cinder/scheduler.log
user=cinder
redirect_stderr=true
stdout_logfile=/var/log/contrail/cinder-scheduler-supervisor-stdout.log
stderr_logfile=/var/log/contrail/cinder-scheduler-supervisor-stderr.log
priority=440
autostart=true
killasgroup=true
stopasgroup=true
stopsignal=KILL
exitcodes=0
